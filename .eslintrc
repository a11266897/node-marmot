ecmaFeatures:
  modules: true
rules:
  indent: [2, 4]
  quotes: [2, single]
  block-scoped-var: 2
  guard-for-in: 1
  no-caller: 2
  no-empty-label: 2
  no-eval: 2
  no-extend-native: 2
  no-floating-decimal: 2
  no-iterator: 2
  no-loop-func: 1
  no-multi-spaces: 2
  no-native-reassign: 2
  no-new-func: 2
  no-new: 0
  no-console: 1
  no-debugger: 1
  no-new-wrappers: 2
  no-proto: 2
  no-with: 2
  radix: 1
  semi: [2, always]
  curly: [2, multi-line]
  strict: [2, global]
env:
  node: true
  es6: true
